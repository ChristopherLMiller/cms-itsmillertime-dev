{
  "name": "cms-itsmillertime-dev",
  "version": "3.8.2",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "changeset": "changeset",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "release": "pnpm build && changeset publish",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "version": "changeset version"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.74.0",
    "@payloadcms/email-resend": "3.74.0",
    "@payloadcms/kv-redis": "^3.74.0",
    "@payloadcms/next": "3.74.0",
    "@payloadcms/payload-cloud": "3.74.0",
    "@payloadcms/plugin-cloud-storage": "3.74.0",
    "@payloadcms/plugin-ecommerce": "^3.74.0",
    "@payloadcms/plugin-mcp": "^3.74.0",
    "@payloadcms/plugin-search": "3.74.0",
    "@payloadcms/plugin-sentry": "3.74.0",
    "@payloadcms/plugin-seo": "3.74.0",
    "@payloadcms/richtext-lexical": "3.74.0",
    "@payloadcms/storage-s3": "3.74.0",
    "@payloadcms/ui": "3.74.0",
    "@sentry/nextjs": "10.38.0",
    "@veiag/payload-cmdk": "^1.0.6",
    "blurhash": "^2.0.5",
    "cross-env": "^10.1.0",
    "date-fns": "^4.1.0",
    "exifreader": "^4.36.1",
    "fast-xml-parser": "^5.3.4",
    "lucide-react": "^0.563.0",
    "next": "^15.5.9",
    "payload": "3.74.0",
    "payload-plugin-webhooks": "^1.1.0",
    "payload-sidebar-plugin": "^1.3.5",
    "pg": "8.18.0",
    "plaiceholder": "^3.0.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "sharp": "0.34.5",
    "slugify": "^1.6.6",
    "word-counting": "^1.1.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/eslintrc": "^3.3.3",
    "@types/node": "^25.2.0",
    "@types/pg": "^8.16.0",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "@typescript/native-preview": "^7.0.0-dev.20260202.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "prettier": "^3.8.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "@sentry/cli"
    ]
  }
}
